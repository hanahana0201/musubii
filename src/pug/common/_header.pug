header.section.is-topcover-header
  .inner-soft
    .grid.is-between.is-middle.is-gap-none
      .col
        a(href=site.url)
          h1.obj.is-logo-header-wrap
            img.obj.is-logo-header(src=path.img+"/"+"logo-set-yoko-musubii.svg" alt="MUSUBii")
      .col
        nav.tabs.is-strong.is-uppercase
          a.tab.is-ghost.is-sm.is-home.is-active.js-tab(href="#") Home
          a.tab.is-ghost.is-sm.js-tab(href="#") Getting Started
          a.tab.is-ghost.is-sm.js-tab(href="#") Documentation
          a.tab.is-ghost.is-sm.js-tab(href="#") Libraries